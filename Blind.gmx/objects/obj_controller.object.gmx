        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (game_over == 0) {
    draw_sprite_stretched(spr_static_materia_x, -1, 20, 10, 30, 30)
    draw_sprite_stretched(spr_tempclock, -1, 70, 10, 30, 30)
    draw_set_color(c_white)
    draw_set_font(ui_number)
    draw_set_halign(fa_left)
 
    draw_text(45, 26, string(obj_character.materia_x))   
    draw_text(95, 26, string(obj_character.clocks))
    
} if (game_over == 1) {
    life = obj_character.health
    clocks = obj_character.clocks
    enemies_destroyed = obj_character.enemies_destroyed
    instance_deactivate_all(true)
    game_over = 2
} if (game_over == 2) {
    points = 0
    points += life*10
    points += clocks*5
    points += enemies_destroyed*10
    draw_set_color(c_white)
    draw_set_font(scores_font)
    draw_background(background_scores, 130, 10);
    draw_set_halign(fa_right)
    draw_text(450, 300, 'Itens Colecionados: ')
    draw_text(450, 350, 'Vida: ')
    draw_text(450, 400, 'Monstros Derrotados: ')
    draw_text(450, 450, 'Pontuação Final: ')

    draw_set_halign(fa_left)
    
    if (global.level_selected == 2) {
    
        num_clocks = 3
        num_enemies = 1
    
        draw_text(450, 300, string(clocks) + '/3')
        draw_text(450, 350, string(life) + "%")
        draw_text(450, 400, string(enemies_destroyed) + '/1')
    //    draw_text(450, 450, string(points))
    } else if (global.level_selected == 3) {
    
        num_clocks = 3
        num_enemies = 1
    
        draw_text(450, 300, string(clocks) + '/3')
        draw_text(450, 350, string(life) + "%")
        draw_text(450, 400, string(enemies_destroyed) + '/2')
      //  draw_text(450, 450, string(points))    
    } else if (global.level_selected == 4) {
    
        num_clocks = 3
        num_enemies = 2
    
        draw_text(450, 300, string(clocks) + '/3')
        draw_text(450, 350, string(life) + "%")
        draw_text(450, 400, string(enemies_destroyed) + '/2')
      //  draw_text(450, 450, string(points))    
    }
    
        
    draw_set_font(ui_number)    
    draw_set_halign(fa_right)
    
    if (global.level_selected == 2)
        draw_text(750, 550, 'Nível completado. #Pressione X para continuar ou O para recomeçar.')
    if (global.level_selected == 3)
        draw_text(750, 550, 'Nível completado. #Pressione X para continuar ou O para recomeçar.')
    if (global.level_selected == 4) {
        draw_set_font(scores_font)
        draw_set_color(c_yellow)
        draw_set_halign(fa_center)
        draw_text(400, 500, 'Parabéns!')
        draw_set_font(scores_font)
        draw_text(400, 550, 'Chegaste ao final da demonstração do Blind.')
    }
    
    if (life == 100 &amp;&amp; enemies_destroyed == num_enemies &amp;&amp; clocks == num_clocks) {
        draw_sprite (spr_star, 0, 460, 450)
        draw_sprite (spr_star, 0, 510, 450)
        draw_sprite (spr_star, 0, 560, 450)
    } else if ((life == 100 &amp;&amp; enemies_destroyed &gt; 0) || (life == 100 &amp;&amp; clocks &gt; 0) || (enemies_destroyed &gt; 0 &amp;&amp; clocks &gt; 0)) {
        draw_sprite (spr_star, 0, 460, 450)
        draw_sprite (spr_star, 0, 510, 450)
    } else if (life == 100 || enemies_destroyed &gt; 0 || clocks &gt; 0) {
        draw_sprite (spr_star, 0, 460, 450)
    }


} 
</string>
